#include<stdio.h>
int main()
{
    int t, a, b, i, c=1, sum, h;

    scanf("%d", &t);
    while(t--)
    {
        scanf("%d %d", &a, &b);
        if(a%2==0)
            a+=1;
        if(b%2==0)
            b-=1;
        h=(b-a)/2+1;
        sum=(a+b)*h/2;
        printf("Case %d: %d\n", c, sum);
        c++;
    }
    return 0;
}
