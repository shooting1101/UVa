#include<stdio.h>
int main()
{
    int t, a, b, c;
    scanf("%d", &t);
    while(t--)
    {
        scanf("%d %d", &a, &b);
        c=a==b?0:(a>b?1:-1);
        if(!c)
            printf("=\n");
        else
        {
            if(c>0)
                printf(">\n");
            else
                printf("<\n");
        }
    }
    return 0;
}
